{
    "name": "nodeboard",
    "version": "1.3.0",
    "description": "Nodeboard is An Open-Source Forum Platform",
    "main": "build/app.js",
    "scripts": {
        "prod": "export NODE_ENV=prod && node build/app.js",
        "start": "concurrently npm:start:*",
        "start:build": "tsc -w",
        "start:dev": "export NODE_ENV=dev && nodemon build/app.js",
        "lint": "eslint ./src --fix --ext .ts",
        "swagger": "swagger-cli bundle -r src/api/swagger/openapi3.yaml --o src/api/swagger/swagger.yaml --t yaml",
        "test": "export NODE_ENV=test && jest",
        "test:watch": "export NODE_ENV=test && jest --watchAll --verbose --coverage"
    },
    "keywords": [
        "nodeboard",
        "voip",
        "messaging"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AlSnake/nodeboard.git"
    },
    "author": "Al Snake",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/AlSnake/nodeboard/issues"
    },
    "homepage": "https://github.com/AlSnake/nodeboard#readme",
    "dependencies": {
        "@sapphire/snowflake": "^3.0.1",
        "app-root-path": "^3.0.0",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.6",
        "dotenv": "^14.2.0",
        "express": "^4.17.2",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^6.1.0",
        "express-validator": "^6.14.0",
        "jsonwebtoken": "^8.5.1",
        "mongodb": "^4.3.1",
        "mongoose": "^6.1.7",
        "nodemailer": "^6.7.2",
        "swagger-ui-express": "^4.3.0",
        "winston": "^3.4.0",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@faker-js/faker": "^6.0.0-alpha.5",
        "@types/app-root-path": "^1.2.4",
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.10",
        "@types/nodemailer": "^6.4.4",
        "@types/supertest": "^2.0.11",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/yamljs": "^0.2.31",
        "concurrently": "^7.0.0",
        "eslint": "^8.7.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.4.7",
        "nodemon": "^2.0.15",
        "supertest": "^6.2.2",
        "swagger-cli": "^4.0.4",
        "typescript": "^4.5.5"
    }
}
